{
  "name": "gc2",
  "description": "CLI tool for GC2",
  "version": "2022.9.3",
  "author": "Martin HÃ¸gh @mapcentia",
  "bin": {
    "gc2": "./bin/run"
  },
  "bugs": {
    "url": "https://github.com/mapcentia/gc2-cli/issues"
  },
  "dependencies": {
    "@oclif/command": "^1.8.16",
    "@oclif/core": "^1.9.3",
    "@oclif/plugin-help": "^5.1.12",
    "@oclif/plugin-update": "^3.0.0",
    "@types/configstore": "^4.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/node-fetch": "^2.5.8",
    "@types/through2": "^2.0.36",
    "@types/url-parse": "^1.4.3",
    "base64url": "^3.0.1",
    "chalk": "^4.1.0",
    "cli-ux": "^5.5.1",
    "configstore": "^5.0.1",
    "inquirer": "^7.3.3",
    "node-fetch": "^2.6.1",
    "through2": "^3.0.2",
    "tslib": "^1.14.1",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@aws-sdk/client-s3": "^3.113.0",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.14",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.51",
    "aws-sdk": "^2.1159.0",
    "chai": "^4.2.0",
    "globby": "^10.0.2",
    "mocha": "^5.2.0",
    "nyc": "^14.1.1",
    "ts-node": "^8.10.2",
    "tslint": "^5.20.1",
    "typescript": "^3.9.7"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/mapcentia/gc2-cli",
  "keywords": [
    "oclif"
  ],
  "license": "GPL",
  "main": "lib/index.js",
  "oclif": {
    "additionalHelpFlags": [
      "-h"
    ],
    "additionalVersionFlags": [
      "-v"
    ],
    "topicSeparator": " ",
    "commands": "./lib/commands",
    "bin": "gc2",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-update"
    ],
    "update": {
      "s3": {
        "bucket": "gc2-cli"
      }
    },
    "macos": {
      "identifier": "io.gc2.cli"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mapcentia/gc2-cli.git"
  },
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "posttest": "tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "nyc --extension .ts mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts",
  "directories": {
    "test": "test"
  }
}

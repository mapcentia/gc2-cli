    "@oclif/plugin-help": "^2.2.1",
    "@oclif/plugin-update": "^1.3.9",
    "@types/configstore": "^4.0.0",
    "@types/inquirer": "^6.5.0",
    "@types/node-fetch": "^2.5.3",
    "@types/through2": "^2.0.34",
    "@types/url-parse": "^1.4.3",
    "chalk": "^4.1.0",
    "cli-ux": "^5.3.3",
    "configstore": "^5.0.0",
    "inquirer": "^7.0.0",
    "node-fetch": "^2.6.0",
    "through2": "^3.0.1",
    "tslib": "^1.10.0",
    "url-parse": "^1.4.7"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.22.2",
    "@oclif/test": "^1.2.5",
    "@oclif/tslint": "^3.1.1",
    "@types/chai": "^4.2.5",
    "@types/mocha": "^5.2.7",
    "@types/node": "^10.17.5",
    "aws-sdk": "^2.632.0",
